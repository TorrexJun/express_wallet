{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "serve": "babel-node --presets env server.ts",
    "build:test": "vue-tsc --noEmit && vite build --mode test",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ./src/**/*.{vue,.js,.ts,.jsx,.tsx} --fix",
    "format": "prettier --write src/**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.16",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "animate.css": "^4.1.1",
    "ant-design-vue": "^3.2.13",
    "axios": "^0.27.2",
    "chartist": "^1.2.1",
    "core-js": "^3.25.5",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.2",
    "express": "^4.18.1",
    "file-saver": "^2.0.5",
    "hfmath": "^0.0.2",
    "html-to-image": "^1.10.8",
    "lodash": "^4.17.21",
    "mitt": "^3.0.0",
    "nanoid": "^4.0.0",
    "node-sass": "^7.0.1",
    "pinia": "^2.0.22",
    "pptxgenjs": "^3.11.0",
    "prosemirror-commands": "^1.3.1",
    "prosemirror-dropcursor": "^1.6.0",
    "prosemirror-gapcursor": "^1.3.1",
    "prosemirror-history": "^1.3.0",
    "prosemirror-inputrules": "^1.2.0",
    "prosemirror-keymap": "^1.2.0",
    "prosemirror-model": "^1.18.1",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-schema-list": "^1.2.2",
    "prosemirror-state": "^1.4.2",
    "prosemirror-view": "^1.29.0",
    "qiankun": "^2.7.5",
    "register-service-worker": "^1.7.2",
    "sass-loader": "^13.1.0",
    "svg-arc-to-cubic-bezier": "^3.2.0",
    "svg-pathdata": "^6.0.3",
    "tinycolor2": "^1.4.2",
    "vite-plugin-vconsole": "^1.2.2",
    "vue": "^3.2.37",
    "vue-router": "4",
    "vuedraggable": "^2.24.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "babel-preset-env": "^1.7.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.5.1",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "sass": "^1.55.0",
    "typescript": "*",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-icons": "^0.14.9",
    "unplugin-vue-components": "^0.22.7",
    "vite": "^3.0.7",
    "vue-tsc": "^0.39.5"
  },
  "lint-staged": {
    "src/**/*.{js,vue,ts,tsx,jsx}": [
      "yarn lint",
      "yarn format"
    ],
    "run/*": [
      "git rm --cached"
    ]
  }
}
